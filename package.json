{
  "name": "oddworks",
  "version": "2.0.0",
  "description": "An example Oddworks API written in Express.js",
  "main": "server.js",
  "bin": {
    "oddworks": "cli/index.js"
  },
  "scripts": {
    "env": "if [ ! -f '.env' ]; then cp .env.sample .env; fi",
    "setup": "npm link && npm run env",
    "dev": "nodemon server.js -e js,json",
    "test": "NODE_ENV=test npm run lint && npm run tape",
    "tape": "NODE_ENV=test tape 'test/**/*_test.js' | tap-spec",
    "lint": "xo",
    "start": "node index.js -e js,json",
    "sec": "./node_modules/.bin/nsp check -o json --warn-only"
  },
  "engines": {
    "npm": "2.11.x",
    "node": "4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oddchannels/oddworks.git"
  },
  "author": "Odd channels",
  "bugs": {
    "url": "https://github.com/oddchannels/odd-api/issues"
  },
  "homepage": "https://github.com/oddchannels/odd-api",
  "dependencies": {
    "async": "^1.4.2",
    "bluebird": "^3.3.4",
    "body-parser": "^1.14.1",
    "boom": "^2.9.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "compression": "^1.5.2",
    "deep-freeze": "0.0.1",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "express-real-ip": "^1.0.0",
    "express-slash": "^2.0.1",
    "fakeredis": "^1.0.3",
    "filepath": "^1.0.0",
    "glob": "^5.0.15",
    "header-parser": "^1.0.0",
    "hiredis": "^0.4.1",
    "inflection": "^1.7.1",
    "interpolate": "^0.1.0",
    "jsonwebtoken": "^5.0.5",
    "keymirror": "^0.1.1",
    "lodash": "^3.10.1",
    "lru-cache": "^4.0.0",
    "mixpanel": "^0.3.2",
    "moment": "^2.11.1",
    "mongo-uri": "^0.1.2",
    "morgan": "^1.6.1",
    "oddcast": "^2.0.0",
    "optimist": "^0.6.1",
    "query-string": "^3.0.0",
    "redis": "^2.4.2",
    "redis-search": "0.0.1",
    "universal-analytics": "^0.3.11",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "mock-express-request": "^0.1.1",
    "mock-express-response": "^0.1.1",
    "nodemon": "^1.3.7",
    "nsp": "^2.2.0",
    "sinon": "^1.17.3",
    "supertest": "^1.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.0.1",
    "xo": "^0.9.0"
  },
  "xo": {
    "envs": [
      "node",
      "es6"
    ]
  },
  "license": "Apache-2.0"
}
