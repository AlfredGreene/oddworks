{
	"id": "odd-networks",
	"type": "channel",
	"title": "Odd Networks",
	"features": {
		"authentication": {
			"enabled": true
		},
		"sharing": {
			"enabled": true,
			"text": "Watch the @oddnetworks show Live on mobile and TV connected devices!"
		},
		"metrics": {
			"enabled": true,
			"appInit": {
				"action": "app:init",
				"enabled": true
			},
			"viewLoad": {
				"action": "view:load",
				"enabled": true
			},
			"videoPlay": {
				"action": "video:play",
				"enabled": true
			},
			"videoPlaying": {
				"action": "video:playing",
				"enabled": false,
				"interval": 30000
			},
			"videoStop": {
				"action": "video:stop",
				"enabled": true
			},
			"videoError": {
				"action": "video:error",
				"enabled": true
			}
	}
	}
}
